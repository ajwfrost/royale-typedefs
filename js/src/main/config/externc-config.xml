        <locale/>
        
        <library-path/>
        
        <!--
        <namespaces>
            <namespace>
                <uri>library://ns.adobe.com/flex/spark</uri>
                <manifest>manifest.xml</manifest>
            </namespace>
        </namespaces>
        -->
        
        <source-path>
            <path-element>src</path-element>
        </source-path>
        
        <warn-no-constructor>false</warn-no-constructor>
    </compiler>
    
    <external>
        <path-element>../javascript/missing.js</path-element>
